@article{Blanco1997,
abstract = {The use of refuse by breeding, floating, and migrating Black Kites (Milvus migrans) was studied near a large garbage dump in Madrid, Spain. Refuse was an important food resource for nonbreeding Black Kites, especially migrants that fed almost exclusively at the garbage dump. The dump was only of secondary and variable importance for kites during the breeding season. Pairs of breeding kites nesting in nearby wooded areas foraged mainly on a wide variety of wild prey and only sporadically ate refuse at the dump. Floaters roosted in nearby wooded areas but, unlike breeding pairs, they foraged mainly by scavenging at the dump. Exploitation of food resources other than refuse by breeding kites may be due to their need for large amounts of prey biomass for brood provisioning. Perhaps dumps augment populations of Black Kites by providing rich foraging areas where large numbers of nonbreeding and migratory kites can scavenge for food. {\textcopyright} 1997 The Raptor Research Foundation, Inc.},
author = {Blanco, Guillermo},
file = {:home/felix/Dokumente/studium/Potsdam/Module/Biogeography/Black kite/Latex document/lit/Blanco{\_}1997{\_}Role of refuse as food for migrant, floater and breeding Black Kites (Milvus migrans).pdf:pdf},
journal = {Journal of Raptor Research},
keywords = {Black Kite,Foraging,Garbage dump,Milvus migrans,Refuse},
number = {1},
pages = {71--76},
title = {{Role of refuse as food for migrant, floater and breeding Black Kites (Milvus migrans)}},
volume = {31},
year = {1997}
}
