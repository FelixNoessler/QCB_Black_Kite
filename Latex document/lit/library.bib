@article{Panuccio2014,
abstract = {The nominal subspecies of the Black Kite is a summer resident in Europe and Asia that winters mostly in western Africa, although numbers of birds wintering in the Mediterranean area are increasing. During migrations, tens of thousands are observed migrating through the Strait of Gibraltar between Spain and Morocco, along the eastern side of the Black Sea, and in the Middle East, while substantial numbers cross the central Mediterranean and the Bosphorus. This paper provides a review of research concerning migration and its relationship with foraging behaviour in a circannual perspective. In particular, research made both by satellite tracking and by visual observations suggests a more evident time-selected migration during autumn rather than spring. Moreover, differences in timing occurring among different flyways could be explained either by different rates of intra-specific competition in areas with different breeding density and/or by different distances between wintering and breeding grounds. {\textcopyright} 2013 Dipartimento di Biologia, Universit{\`{a}} di Firenze, Italia.},
annote = {migratory patterns},
author = {Panuccio, M. and Agostini, N. and Mellone, U. and Bogliani, G.},
doi = {10.1080/03949370.2013.812147},
file = {:home/felix/Dokumente/studium/Potsdam/Module/Biogeography/Black kite/Latex document/lit/Panuccio et al.{\_}2014{\_}Circannual variation in movement patterns of the Black Kite (Milvus migrans migrans) A review.pdf:pdf},
journal = {Ethology Ecology and Evolution},
keywords = {Black Kite,Mediterranean Sea,Milvus migrans migrans,dispersal behaviour,migration},
number = {1},
pages = {1--18},
publisher = {Taylor {\&} Francis},
title = {{Circannual variation in movement patterns of the Black Kite (Milvus migrans migrans): A review}},
volume = {26},
year = {2014}
}
